{
  "name": "@vardario/svelte-i18next",
  "version": "0.1.2",
  "description": "",
  "license": "MIT",
  "author": "Sahin Vardar",
  "type": "module",
  "main": "lib/index.js",
  "bin": {
    "svelte-i18next": "./bin/cli.js"
  },
  "files": [
    "/lib",
    "!/**/*.map",
    "!/**/*.test.*"
